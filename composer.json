{
    "name": "louis/laravel-linepay",
    "description": "the package is linepay's payment api",
    "type": "library",
    "require": {
        "php": ">=7.4",
        "illuminate/config": ">=8.0",
        "illuminate/contracts": ">=8.0",
        "illuminate/log": ">=8.0",
        "illuminate/support": ">=8.0",
        "nesbot/carbon": ">=1.0",
        "guzzlehttp/guzzle": "^7.0",
        "yidas/line-pay-sdk": "^3.7"
    },
    "require-dev": {
        "mockery/mockery": "^1.3",
        "phpunit/phpunit": "^9.6",
        "orchestra/testbench": "^6.0"
    },
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Louis\\LaravelLinepay\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Louis\\LaravelLinepay\\Tests\\": "tests"
        }
    },
    "authors": [
        {
            "name": "Louis",
            "email": "louis.zhan.tidy@gmail.com"
        }
    ],
    "extra": {
        "laravel": {
            "providers": [
                "Louis\\LaravelLinepay\\LinepayServiceProvider"
            ],
            "aliases": {
                "Linepay": "Louis\\LaravelLinepay\\Facades\\Linepay"
            }
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "@php vendor/bin/testbench package:discover --ansi"
        ]
    }
}
